    <item android:id="@android:id/progress">
        <shape
            android:innerRadiusRatio="2.5"
            android:shape="ring"
            android:thicknessRatio="25.0" >
            <gradient
                android:centerColor="@color/sky_blue"
                android:endColor="@color/sky_blue"
                android:startColor="@color/sky_blue"
                android:type="sweep" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape
            android:innerRadiusRatio="2.5"
            android:shape="ring"
            android:thicknessRatio="25.0" >
            <gradient
                android:centerColor="@color/dark_green"
                android:endColor="@color/dark_green"
                android:startColor="@color/dark_green"
                android:type="sweep" />
        </shape>
    </item>
</layer-list>